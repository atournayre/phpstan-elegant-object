{
    "name": "atournayre/phpstan-elegant-object",
    "description": "Custom PHPStan rules for enforcing elegant object design",
    "type": "phpstan-extension",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Atournayre\\PHPStan\\ElegantObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atournayre\\PHPStan\\ElegantObject\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2.22",
        "phpstan/phpstan": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0"
    },
    "extra": {
        "phpstan": {
            "includes": [
                "phpstan.neon"
            ]
        }
    }
}
